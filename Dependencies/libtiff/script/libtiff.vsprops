<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="libtiff"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(libtiff)\include"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="libtiff.lib"
		AdditionalLibraryDirectories="$(libtiff)\lib"
	/>
	<UserMacro
		Name="libtiff"
		Value="$(SolutionDir)\Dependencies\libtiff"
	/>
</VisualStudioPropertySheet>
